{
  "name": "@stylewind/core",
  "version": "0.0.9",
  "description": "Create components with tailwindcss easily and quickly (inspired by styled-components)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "npm run clean && tsc -w --declaration",
    "build": "chmod +x ./build.sh && ./build.sh",
    "publ": "npm run build && npm publish --access public",
    "pre:publ": "npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonatasge/stylewind.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonatasge/stylewind/issues"
  },
  "homepage": "https://github.com/jonatasge/stylewind#readme",
  "dependencies": {
    "@emotion/is-prop-valid": "^1.2.2",
    "i": "^0.3.7",
    "npm": "^10.4.0",
    "react": ">=16.9.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/react": ">=16.9.56",
    "typescript": "^5.3.3",
    "uglify-js": "^3.17.4"
  }
}
